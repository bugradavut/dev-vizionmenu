#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/vision-menu/node_modules/.pnpm/jest@29.7.0_@types+node@20._90514a2e080b4b37add1be5ce037f8dc/node_modules/jest/bin/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/jest@29.7.0_@types+node@20._90514a2e080b4b37add1be5ce037f8dc/node_modules/jest/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/jest@29.7.0_@types+node@20._90514a2e080b4b37add1be5ce037f8dc/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/vision-menu/node_modules/.pnpm/jest@29.7.0_@types+node@20._90514a2e080b4b37add1be5ce037f8dc/node_modules/jest/bin/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/jest@29.7.0_@types+node@20._90514a2e080b4b37add1be5ce037f8dc/node_modules/jest/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/jest@29.7.0_@types+node@20._90514a2e080b4b37add1be5ce037f8dc/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
