#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/vision-menu/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_5b97c4556f482833539066c262a6e591/node_modules/ts-jest/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_5b97c4556f482833539066c262a6e591/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/vision-menu/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_5b97c4556f482833539066c262a6e591/node_modules/ts-jest/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@_5b97c4556f482833539066c262a6e591/node_modules:/mnt/c/vision-menu/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
